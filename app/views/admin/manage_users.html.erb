<br/>
<%= link_to 'Create new administrator account', new_user_path %>
<br/>
<br/>
<table border="1">
  <caption><h1>Users</h1></caption>
  <tr>
    <th>Name</th>
    <th>Email</th>
    <th></th>
  </tr>
  <% @users.each do |user| %>
      <tr>
        <td> <%= user.first_name+ " " + user.last_name %> </td>
        <td> <%= user.email %> </td>
        <td> <%= link_to 'Delete user', user,
                    :confirm => 'Are you sure to delete this user?',
                    :method => :delete %> </td>
      </tr>
  <% end %>
</table>
